using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class MSumofFact
    {
        static void Main(string[] args)
        {
            MSumofFact f = new MSumofFact();
            Console.WriteLine(f.FactorsSum(12));
        }
            public int FactorsSum(int num)
            {
                int sum = 0;
                for (int i = 1; i <= num; i++)
                {
                    if (num % i == 0)
                    {
                       sum=sum+i;
                    }
                }
                return sum;
            }
    }
}
