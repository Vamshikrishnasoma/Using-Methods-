using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class MFactorsSum
    {
        static void Main(string[] args)
        {
            MFactorsSum f = new MFactorsSum();
            f.FactorsSum(12);


        }
        public int FactorsSum(int num)
        {
            int count = 0;
            for(int i=1;i<=num;i++)
            {
                if(num%i==0)
                {
                    count++;
                }
            }
            return count;
        }
    }
}
